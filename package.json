{
  "name": "bun-react-template",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=development bun --watch src/server.tsx",
    "build": "bun ./src/build.ts",
    "db:sqlite:init": "bun ./src/initSQLite.ts",
    "compile:linux:modern": "NODE_ENV=production bun build --compile ./src/server.tsx --sourcemap --target=bun-linux-x64-modern  --minify --define:process.env.NODE_ENV='\"production\"' --env='BUN_PUBLIC_*' --outfile benchmarks-linux-modern --production",
    "compile:macos:arm64": "NODE_ENV=production bun build --compile ./src/server.tsx --sourcemap --target=bun-darwin-arm64  --minify --define:process.env.NODE_ENV='\"production\"' --env='BUN_PUBLIC_*' --outfile benchmarks-macos-arm64 --production",
    "compile:windows:modern": "NODE_ENV=production bun build --compile ./src/server.tsx --sourcemap --target=bun-windows-x64-modern  --minify --define:process.env.NODE_ENV='\"production\"' --env='BUN_PUBLIC_*' --outfile benchmarks.exe --production",
    "start": "NODE_ENV=production bun src/server.tsx"
  },
  "dependencies": {
    "lucide-react": "0.546.0",
    "react": "19.2.0",
    "react-aria-components": "^1.13.0",
    "react-dom": "19.2.0",
    "valtio": "^2.1.8"
  },
  "devDependencies": {
    "@types/bun": "1.3.0",
    "@types/react": "19.2.0",
    "@types/react-dom": "19.2.0"
  }
}
