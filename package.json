{
  "name": "bun-react-template",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bun --watch src/index.tsx",
    "build": "bun build ./src/index.tsx --sourcemap --target=bun --compile --minify --define:process.env.NODE_ENV='\"production\"' --env='BUN_PUBLIC_*' --outfile benchmarks",
    "start": "NODE_ENV=production bun src/index.tsx"
  },
  "dependencies": {
    "react": "19.2.0",
    "react-dom": "19.2.0"
  },
  "devDependencies": {
    "@types/bun": "1.3.0",
    "@types/react": "19.2.0",
    "@types/react-dom": "19.2.0"
  }
}
