{
  "name": "bun-react-template",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=development bun --watch src/server.tsx",
    "build": "bun ./src/build.ts",
    "db:sqlite:init": "bun ./src/initSQLite.ts",
    "compile": "NODE_ENV=production bun build ./src/server.tsx --sourcemap --target=bun --compile --minify --define:process.env.NODE_ENV='\"production\"' --env='BUN_PUBLIC_*' --outfile benchmarks --produc",
    "start": "NODE_ENV=production bun src/server.tsx"
  },
  "dependencies": {
    "lucide-react": "0.546.0",
    "react": "19.2.0",
    "react-dom": "19.2.0"
  },
  "devDependencies": {
    "@types/bun": "1.3.0",
    "@types/react": "19.2.0",
    "@types/react-dom": "19.2.0"
  }
}
